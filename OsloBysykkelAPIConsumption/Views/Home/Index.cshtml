@model OsloBysykkelAPIConsumption.Models.RootObject

@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Oslo bysykkel</title>

    <link rel="stylesheet" type="text/css" href="~/css/site.css" />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="~/js/site.js"></script>

    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>
<body>
    <div class="text-center">
        <h1 class="display-4">Oslo BySykkel</h1>

        <p>Liste over alle sykkel stativene i Oslo.</p>

        <input type="button" value="Oppdater Tabellen" onclick="SendData()" />
        <table id="mytable">
            <thead>
                <tr>
                    @*<th class="smallCell">Stations ID</th>*@
                    <th class="mediumCell">Stations</th>
                    <th class="smallCell">Ledige Sykler</th>
                    <th class="smallCell">Tilgjengelige Låser</th>
                </tr>
            </thead>
            <tbody id="tablebody">
            </tbody>
        </table>
    </div>
</body>
</html>